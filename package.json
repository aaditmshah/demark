{
  "name": "@demark/parser",
  "version": "1.1.0",
  "description": "The official Demark language specification and parser.",
  "keywords": [
    "demark",
    "markup",
    "parser"
  ],
  "homepage": "https://github.com/aaditmshah/demark",
  "bugs": "https://github.com/aaditmshah/demark/issues",
  "license": "MIT",
  "author": "Aadit M Shah <aaditmshah@aadit.codes> (https://aadit.codes/)",
  "funding": [
    {
      "type": "individual",
      "url": "https://aadit.codes/sponsor"
    },
    {
      "type": "github",
      "url": "https://github.com/sponsors/aaditmshah"
    },
    {
      "type": "opencollective",
      "url": "https://opencollective.com/aaditmshah"
    },
    {
      "type": "patreon",
      "url": "https://www.patreon.com/aaditmshah"
    },
    {
      "type": "ko-fi",
      "url": "https://ko-fi.com/aaditmshah"
    },
    {
      "type": "buymeacoffee",
      "url": "https://www.buymeacoffee.com/aaditmshah"
    }
  ],
  "source": "src/index.ts",
  "targets": {
    "main": {
      "optimize": true
    },
    "module": {
      "optimize": true
    }
  },
  "files": [
    "dist/*"
  ],
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/index.d.ts",
  "repository": "github:aaditmshah/demark",
  "scripts": {
    "clean": "git clean -fdx",
    "lint": "eslint .",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "test": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "prebuild": "rm -rf .parcel-cache dist",
    "build": "parcel build --no-source-maps",
    "ci": "concurrently --kill-others-on-fail 'yarn prettier:check' 'yarn lint' 'yarn typecheck' 'yarn test' 'yarn build'",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@unicode/unicode-15.0.0": "1.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@babel/preset-env": "7.19.3",
    "@babel/preset-typescript": "7.18.6",
    "@next/eslint-plugin-next": "12.2.5",
    "@parcel/packager-ts": "2.7.0",
    "@parcel/transformer-typescript-types": "2.7.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/jest": "29.1.1",
    "@types/node": "18.7.23",
    "@typescript-eslint/eslint-plugin": "5.35.1",
    "@typescript-eslint/parser": "5.35.1",
    "concurrently": "7.4.0",
    "eslint": "8.22.0",
    "eslint-config-ivory": "0.1.3",
    "eslint-import-resolver-typescript": "3.5.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.8.7",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-promise": "6.0.1",
    "eslint-plugin-react": "7.31.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unicorn": "43.0.2",
    "fast-check": "3.1.4",
    "gitmoji-cli": "7.0.1",
    "husky": "8.0.1",
    "is-ci": "3.0.1",
    "jest": "29.1.2",
    "parcel": "2.7.0",
    "prettier": "2.7.1",
    "semantic-release": "19.0.5",
    "semantic-release-gitmoji": "1.4.5",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
